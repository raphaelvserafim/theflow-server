    "@types/request-promise": "4.1.45",
    "@types/sinon": "9.0.4",
    "@types/sinon-chai": "3.2.4",
    "@types/supertest": "2.0.10",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "concurrently": "5.3.0",
    "mocha": "8.1.1",
    "nodemon": "1.19.4",
    "nyc": "15.1.0",
    "rimraf": "3.0.0",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
