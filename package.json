{
  "name": "base-server-ts",
  "version": "1.0.0",
  "description": "Base Server TS.",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "prepare": "ts-patch install -s",
    "prepare:db": "ts-node src/database/Synchronize.ts",
    "barrels": "barrelsby --config .barrelsby.json"
  },
  "dependencies": {
    "@raphaelvserafim/google-authenticator": "^1.0.3",
    "@sendgrid/mail": "^8.1.0",
    "@tsed/ajv": "^7.67.4",
    "@tsed/common": "^7.67.4",
    "@tsed/core": "^7.67.4",
    "@tsed/di": "^7.67.4",
    "@tsed/engines": "^7.67.4",
    "@tsed/exceptions": "^7.67.4",
    "@tsed/json-mapper": "^7.67.4",
    "@tsed/logger": "^6.7.5",
    "@tsed/logger-file": "^6.7.5",
    "@tsed/openspec": "^7.67.4",
    "@tsed/platform-cache": "^7.67.4",
    "@tsed/platform-exceptions": "^7.67.4",
    "@tsed/platform-express": "^7.67.4",
    "@tsed/platform-log-middleware": "^7.67.4",
    "@tsed/platform-middlewares": "^7.67.4",
    "@tsed/platform-params": "^7.67.4",
    "@tsed/platform-response-filter": "^7.67.4",
    "@tsed/platform-views": "^7.67.4",
    "@tsed/schema": "^7.67.4",
    "@tsed/swagger": "^7.67.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/swagger-schema-official": "2.0.21",
    "ajv": "^8.12.0",
    "axios": "^0.21.1",
    "barrelsby": "^2.8.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "concurrently": "5.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "email-validator": "^2.0.4",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "mysql2": "^3.6.5",
    "node-uuid": "1.4.8",
    "sequelize": "^6.37.1",
    "sharp": "^0.31.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript-transform-paths": "^3.4.6",
    "ws": "^8.16.0"
  },
